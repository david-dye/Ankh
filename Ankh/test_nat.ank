	// This is 160 digits long. It requires:
	// 2^x > 10^160 => x > 160 * log_2(10) => x > 532 => g_max_nat_bits must be at least 1024
	1234567890098765432112345678900987654321123456789009876543211234567890098765432112345678900987654321123456789009876543211234567890098765432112345678900987654321n;
}

nat foo() {
	bar();
}


