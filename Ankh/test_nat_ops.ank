
set_max_nat_bits(256);

nat max(nat x, nat y) {
	if (x > y) {
		x;
	}
	else {
		y;
	}
}

nat add(nat x, nat y) {
	x + y;
}

nat add10(nat x, nat y) {
	y + 5 + x + 5n;
}

nat sub(nat x, nat y) {
	x - y;
}

nat mul(nat x, nat y) {
	x * y;
}

nat mod(nat x, nat y) {
	x % y;
}

nat assign(nat x) {
	nat y;
	y = x;
	y;
}

int add_in_place(nat x, nat y) {
	x = x + y;
	0;
}

nat bsl(nat x, int y) {
	x << y;
}

nat bsr(nat x, int y) {
	x >> y;
}

nat bwxor(nat x, nat y) {
	x ^ y;
}

nat bwor(nat x, nat y) {
	x | y;
}

nat bwand(nat x, nat y) {
	x & y;
}